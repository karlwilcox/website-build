<script>
function setGallery() {
   const imgs = [
      {% for img in site.data.artwork -%}
               "/{{ img.thumb_url }}",
      {%- endfor %}
   ];
   var entries = [];
   var num = 1;
   while (num <= 8) {
     var chosen = Math.floor(Math.random() * imgs.length);
       if (entries.includes(chosen)) {
        continue;
       } // else
       entries.push(chosen);
       document.getElementById("img" + num).setAttribute('src', imgs[chosen]);
       num += 1;
   }
}
setGallery();
</script>
